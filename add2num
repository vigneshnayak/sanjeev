
#include<stdio.h>
int getinput(int *a,int *b);
int add(int,int);
int output(int);
int main()
{
int a,b,c;
getinput(&a,&b);
c=add(a,b);
output(c);
}
int getinput(int *a,int *b)
{
printf("enter the value of a & b ");
scanf("%d%d",a,b);
}
int add(int a,int b)
{
int c;
c=a+b;
return c;
}
int output(int c)
{
printf("%d",c);
}
